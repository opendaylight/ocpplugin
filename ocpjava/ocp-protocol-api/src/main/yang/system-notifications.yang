 module system-notifications {
    namespace "urn:opendaylight:ocp:system";
    prefix "ofs";

    organization "FACA";
    contact "FACA Web: <https://sites.google.com/site/facafoxconn/>";

    revision "2015-08-11" {
        description "Model of system messages used in OCP Protocol Library";
    }

    notification disconnect-event {
        description "Disconnect notification";
        leaf info {
            type string;
        }
    }

    notification radioHead-idle-event {
        description "Radiohead-idle notification";
        leaf info {
            type string;
        }
    }

}
